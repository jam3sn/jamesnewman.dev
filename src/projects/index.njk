---
title: Projects
layout: base
templateId: projects
---
<header>
    <h1>Projects</h1>
    <hr>
</header>

<section>
    {%- for project in collections.projects -%}
    <article>
        <a href="{{ project.url }}" alt="{{ project.data.title }}">
            <h2>{{ project.data.title }}</h2>
        </a>

        <p>{{ project.data.summary }}</p>
    </article>
    {%- endfor -%}
</section>