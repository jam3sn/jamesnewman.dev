---
layout: base
templateId: blog-post
---
<header>
    <h1>{{ title }}</h1>
    <nav>
        <date>{{ date.getDate() }}/{{ date.getMonth()+1 }}/{{ date.getFullYear() }}</date>
    </nav>
    <hr />
</header>

<section class="content">
    {{ content | safe }}
</section>

<footer>
    <hr>
    <h4>
        <a href="/blog">Back to blog</a>
    </h4>
</footer>