{
  "name": "jamesnewman",
  "version": "1.0.0",
  "description": "jamesnewman.dev site",
  "scripts": {
    "watch": "npm run watch:eleventy & npm run watch:styles",
    "watch:eleventy": "eleventy --serve --watch",
    "watch:styles": "sass src/styles/main.scss src/assets/styles.css --watch  --no-source-map",
    "styles": "sass src/styles/main.scss src/assets/styles.css --style=compressed --no-source-map",
    "minify-js": "minify src/js/site.js > src/assets/site.min.js",
    "build": "npm run styles && npm run minify-js && eleventy"
  },
  "keywords": [],
  "author": "James Newman",
  "license": "ISC",
  "dependencies": {
    "clean-css": "^4.2.3"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "minify": "^5.1.1",
    "mustache": "^4.0.1",
    "sass": "^1.26.10"
  }
}
