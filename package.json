{
  "name": "jamesnewman",
  "version": "1.0.0",
  "description": "jamesnewman.dev site",
  "scripts": {
    "watch": "npm run watch:eleventy & npm run watch:styles",
    "watch:eleventy": "eleventy --serve --watch",
    "watch:styles": "sass src/styles/main.scss src/assets/styles.css --watch  --no-source-map",
    "styles": "sass src/styles/main.scss src/assets/styles.css --style=compressed --no-source-map",
    "build": "npm run styles && eleventy"
  },
  "keywords": [],
  "author": "James Newman",
  "license": "ISC",
  "dependencies": {
    "clean-css": "^4.2.3"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-plugin-rss": "^1.0.9",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "mustache": "^4.0.1",
    "sass": "^1.26.10"
  }
}
